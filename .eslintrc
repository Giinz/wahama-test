{
    "extends":["react-app","prettier"],
    "plugins": ["react","prettier"],
    "rules": {
        "prettier/prettier":[
            "warn",
            {
                "arrowParens": "always",
                "trailingComma": "none",
                "tabWidth": 2,
                "endOfLine": "auto",
                "singleQuote": true,
                "printWidth":120,
                "jsxSingleQuote": true
            }
        ]
    }
}